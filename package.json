{
  "name": "rhm",
  "version": "0.7.0",
  "description": "Redux Helpers Middleware (RHM). Yet another set of tools to deal with sync and async actions in Redux + some extras",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "rimraf dist/* && webpack --mode production",
    "test": "mocha --require babel-polyfill --require @babel/register --recursive \"./src/**/*.test.js\" --watch"
  },
  "keywords": [
    "Redux",
    "Helpers",
    "Actions",
    "Async"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ludekarts/rhm.git"
  },
  "author": "Wojciech Ludwin, ludekarts@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  }
}
